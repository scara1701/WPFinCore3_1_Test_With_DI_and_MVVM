<Window x:Class="CoreTest.MyWPFGUI.Dialogs.SelectObjectDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CoreTest.MyWPFGUI.Dialogs"
        mc:Ignorable="d"
        Title="SelectObjectDialog" Height="450" Width="800">
    <StackPanel>
        <ListBox ItemsSource="{Binding StringList}" SelectedItem="{Binding SelectedObjectInList}" Height="150" />
        <Button x:Name="btnOk" Content="Ok" Command="{Binding CloseCommandButton}"/>
    </StackPanel>
</Window>
